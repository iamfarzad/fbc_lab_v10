{
  "name": "fbc_lab_v10",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "dev:server": "tsx watch server/live-server.ts",
    "start:server": "tsx server/live-server.ts",
    "build:server": "tsc --project tsconfig.server.json",
    "dev:api": "vercel dev",
    "dev:api:3002": "PORT=3002 vercel dev --yes --listen 3002",
    "dev:api:local": "tsx api-local-server.ts",
    "dev:all": "concurrently -n frontend,websocket -c blue,green \"pnpm dev\" \"pnpm dev:server\"",
    "dev:local:full": "concurrently \"pnpm dev:all\" \"pnpm dev:api:local\"",
    "dev:all:clean": "pnpm install && pnpm dev:all",
    "check:secrets": "node scripts/check-secrets.js",
    "check:circular": "node scripts/check-circular-deps.js",
    "check:unused": "node scripts/check-unused-exports.js",
    "check:naming": "node scripts/check-naming-consistency.js",
    "check:all": "pnpm check:secrets && pnpm type-check && pnpm lint && pnpm check:circular && pnpm check:unused && pnpm check:naming",
    "test:e2e:guide": "node scripts/test-browser-e2e.js",
    "verify:deployment:guide": "node scripts/verify-deployment.js",
    "logs:local": "node scripts/monitor-logs.js local",
    "logs:prod": "node scripts/monitor-logs.js production",
    "status": "node scripts/update-status.js",
    "status:check": "cat PROJECT_STATUS.md | head -30",
    "verify:context": "node scripts/verify-context-system.js",
    "generate:prompts": "node scripts/generate-agent-prompts.js",
    "monitor:phase-1": "node scripts/monitor-agents.js phase-1",
    "monitor:phase-2": "node scripts/monitor-agents.js phase-2",
    "monitor:phase-3": "node scripts/monitor-agents.js phase-3",
    "import:file": "node scripts/import-file.js",
    "validate:phase-1": "pnpm check:all && pnpm build",
    "validate:phase-2": "pnpm check:all && pnpm build",
    "validate:phase-3": "pnpm check:all && pnpm build",
    "validate:phase-4": "pnpm check:all && pnpm build",
    "validate:phase-5": "pnpm check:all && pnpm build",
    "validate:phase-6": "pnpm check:all && pnpm build",
    "validate:phase-7": "pnpm check:all && pnpm build:server",
    "validate:phase-8": "pnpm check:all && pnpm build && pnpm build:server",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.44",
    "@google/genai": "^1.30.0",
    "@juggle/resize-observer": "^3.4.0",
    "@supabase/supabase-js": "^2.86.0",
    "@types/express": "^4.17.25",
    "@types/node": "^24.10.1",
    "@types/uuid": "^11.0.0",
    "@types/ws": "^8.18.1",
    "@vercel/kv": "^3.0.0",
    "@vercel/node": "^5.5.13",
    "ai": "^5.0.104",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "playwright": "^1.57.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  }
}
